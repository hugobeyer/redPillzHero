[application]

config/name="redPillz"
run/main_scene="res://addons/pillzgame/scenes/scatter_pnts_tool.tscn"
run/enable_alt_space_menu=true
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=60
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

SignalBus="res://_scripts/signal_bus.gd"

[debug]

settings/gdscript/max_call_stack=512
gdscript/warnings/unused_variable=0
