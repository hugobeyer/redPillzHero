window/size/viewport_height=1100
window/size/initial_position_type=3
window/size/always_on_top=true
window/size/extend_to_title=true
window/stretch/aspect="keep_width"
display_server/driver="opengl3"
display_server/driver.windows="windows"
display_server/driver.android="android"
display_server/driver.ios="iOS"
display_server/driver.macos="macos"
window/handheld/orientation=1
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://addons/orchestrator/editor/icons/CircleValue.svg"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/pillzgame/plugin.cfg", "res://addons/plugin_refresher/plugin.cfg", "res://addons/thumbstick_plugin/plugin.cfg")

